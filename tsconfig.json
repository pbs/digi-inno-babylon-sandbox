{
  "compilerOptions": {
    "rootDir": "src",
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "es5",
    "module": "esnext",
    "skipLibCheck": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "preserveConstEnums":true,
    "strict": true,
  },
  "exclude": ["node_modules"]
}
